<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.Hibernate.Cache">
	<class name="CustomerCache" table="Customer">
		<meta attribute="class-description">
			This class contains the Customer detail.
		</meta>
		<id name="customerId" type="int" column="CUSTOMERID">
		  <generator class = "increment" />
		</id>
		<property name="customerName" column="CUSTOMERNAME" type="string" />
		<property name="customerCity" column="CUSTOMERCITY" type="string" />
	</class>
</hibernate-mapping>